<html lang="en">
  <%= render "page/header" %>
<body>
  <%= render "page/subhead" %>
<% provide(:title, 'Register') %>
<h2>Sign up</h2>

<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => { :multipart => true }) do |f| %>
  <%= devise_error_messages! %>

  <%= render "/page/profile_fields" %>
  <div><%= f.submit "Sign up" %></div>
<% end %>

<%= render "links" %>
</body>
</html>
