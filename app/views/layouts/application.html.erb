<!DOCTYPE html>
<html lang="en">
  <%= render "layouts/header" %>
<body>
  <%= render "layouts/subhead" %>
  <div id="content">
    <% flash.each do |name, msg| %>
      <%= content_tag :div, msg, :id => "flash_#{name}" %>
    <% end %>
    <%= yield :layout %>
  </div>
  <%= render "layouts/footer" %>
  <div id="basic-modal-content"></div>
</body>
</html>

