<!DOCTYPE html>
<html lang="en">
  <%= render "layouts/header" %>
<body>
  <%= render "layouts/subhead" %>
  <div id="content">
    <% flash.each do |name, msg| %>
      <div class="rightCol">
        <%= content_tag :div, msg.html_safe, :id => "flash_#{name}", :class => 'entry' %>
      </div>
    <% end %>
    <%= yield :layout %>
  </div>
  <%= render "layouts/footer" %>
  <div id="basic-modal-content"></div>
</body>
</html>

